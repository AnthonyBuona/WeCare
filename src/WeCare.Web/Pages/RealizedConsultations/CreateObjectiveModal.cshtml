@page
@model WeCare.Web.Pages.RealizedConsultations.CreateObjectiveModalModel
@using WeCare.Localization
@using Microsoft.AspNetCore.Mvc.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IHtmlLocalizer<WeCareResource> L
@{
    Layout = null;
}
<abp-modal>
     <abp-dynamic-form abp-model="Objective" name="create-objective-form">
    <abp-modal-header title="@L["AddNewObjective"].Value"></abp-modal-header>
    <abp-modal-body>
            <abp-input asp-for="PatientId" />
            <abp-form-content />
    </abp-modal-body>
    <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-dynamic-form>
</abp-modal>